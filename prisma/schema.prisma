generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String      @id
  username      String
  roles         usr_roles[]
  isBlacklisted Boolean     @default(false)
}

enum usr_roles {
  OWNER
  DEVELOPER
  USER
}
